from Tools import *
source1,target1= get_causal_net('./pnml/3 length loop.pnml')
source2,target2= get_causal_net('./pnml/3 length loop.pnml')

dependency1=[]
dependency2=[]
for i in range(0,len(source1)):
    dependency= source1[i]+'-'+target1[i]
    #print(dependency)
    dependency1.append(dependency)

for i in range(0,len(source2)):
    dependency= source2[i]+'-'+target2[i]
    #print(dependency)
    dependency2.append(dependency)

Precision=0
Recall=0
for i in range(0,len(dependency1)):
    for j in range(0,len(dependency2)):
        if dependency1[i]==dependency2[j]:
            Precision=Precision+1
for i in range(0,len(dependency2)):
    for j in range(0,len(dependency1)):
        if dependency2[i]==dependency1[j]:
            Recall=Recall+1
Precision=Precision/len(dependency2)
Recall=Recall/len(dependency1)
print('Precision is',Precision)
print('Recall is',Recall)